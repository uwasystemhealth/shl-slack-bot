
{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "indent": [
          "error",
          2
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ],
      "no-console": [
        "off"
      ],
      "import/named": [
        "off"
      ],
      "no-case-declarations": [
        "off"
      ],
      "eol-last": [
          "error",
          "always"
      ]
  }
}